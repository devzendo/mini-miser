<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="pluginManager" class="uk.me.gumbley.minimiser.pluginmanager.DefaultPluginManager">
		<constructor-arg ref="springLoader" />
		<constructor-arg ref="pluginRegistry" />
	</bean>
	
	<bean id="pluginRegistry" class="uk.me.gumbley.minimiser.pluginmanager.DefaultPluginRegistry" />

</beans>